{
  "name": "schnitzellunch",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.1",
    "@types/node-fetch": "2.5.2",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "apollo-boost": "0.4.4",
    "apollo-client": "2.6.4",
    "apollo-link-http": "1.5.16",
    "apollo-server-lambda": "2.9.6",
    "axios": "0.19.0",
    "axios-hooks": "1.4.1",
    "cheerio": "1.0.0-rc.3",
    "encoding": "0.1.12",
    "faunadb": "2.9.2",
    "graphql-tag": "2.10.1",
